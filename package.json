{
  "name": "mathssyfy",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/DavidCouronne/mathssyfy.git",
  "author": "David Couronn√© <couronne.david@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "lerna run compile",
    "clean": "lerna run clean",
    "update-version": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "check-packages": "yarn clean && yarn compile && yarn test && yarn lint",
    "publish-packages": "./scripts/publish",
    "bootstrap": "lerna bootstrap",
    "boot": "node scripts/bootstrap.js",
    "dev": "yarn tsc && yarn workspace docs dev",
    "build": "yarn tsc && yarn workspace docs build",
    "show-help": "yarn workspace docs show-help",
    "dev:blog": "yarn tsc && yarn workspace blog dev",
    "build:blog": "yarn tsc && yarn workspace blog build",
    "register-vuepress": "lerna exec --scope vuepress -- yarn link",
    "lint": "eslint packages --fix --ext .js,.vue",
    "release": "yarn --pure-lockfile && yarn tsc && node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "test": "node scripts/test.js",
    "tsc": "lerna run tsc"
  },
  "devDependencies": {
    "@types/escape-html": "^0.0.20",
    "@types/fs-extra": "^5.0.4",
    "@types/globby": "^8.0.0",
    "@types/hash-sum": "^1.0.0",
    "@types/lru-cache": "^4.1.1",
    "@types/node": "^10.12.12",
    "@types/semver": "^5.5.0",
    "@vue/component-compiler-utils": "^2.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.12.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-vue-libs": "^3.0.0",
    "inquirer": "^6.2.0",
    "lerna": "^3.10.5",
    "lint-staged": "^8.1.0",
    "minimist": "^1.2.0",
    "typescript": "^3.2.2",
    "yorkie": "^2.0.0"
  }
}
